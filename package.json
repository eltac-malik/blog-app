    "@vitejs/plugin-react-swc": "^3.0.0",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "prettier-plugin-packagejson": "^2.3.0",
    "prettier-plugin-tsconfig": "^0.0.1",
    "typescript": "^4.9.3",
